<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="detteproject.controller.ClientValideDetteController">
    <TableView fx:id="detteTableView" layoutX="20.0" layoutY="20.0" prefHeight="400.0" prefWidth="600.0">
        <columns>
            <TableColumn fx:id="debtorNameColumn" text="Debtor Name" prefWidth="200.0" />
            <TableColumn fx:id="amountColumn" text="Amount" prefWidth="150.0" />
            <TableColumn fx:id="dueDateColumn" text="Due Date" prefWidth="200.0"/>
            <TableColumn fx:id="createat" text="Creation Date" prefWidth="200.0"/>
        </columns>
    </TableView>
</AnchorPane>
